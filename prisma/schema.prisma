// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id  Int @default(autoincrement()) @id
  usn String @unique
  pwd String
}

model Plugin {
  id   String  @id
  name String
  icon String
  desc String
  src  String
}
